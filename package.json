  "keywords": [
    "cli",
    "directory",
    "visualization",
    "generation",
    "analysis",
    "developers",
    "testers"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheAlphamerc/gen-struct/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/TheAlphamerc/gen-struct#readme",
  "dependencies": {
    "@types/node": "^22.13.9",
    "chalk": "^5.4.1",
